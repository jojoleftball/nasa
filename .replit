[agent]
integrations = ["javascript_log_in_with_replit:1.0.0", "javascript_auth_all_persistance:1.0.0", "javascript_database:1.0.0", "javascript_gemini:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
